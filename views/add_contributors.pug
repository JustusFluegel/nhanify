extends signup

block header
    h1(class="pageTitle") #{pageTitle}
block content
  - console.log({pagePl});
  form(class="inputForm" action=`/${playlistType}/playlists/${page}/playlist/${pagePl}/${playlistId}/contributors/add` method="post")
    .formDiv
      label(for="username") username
      input(id="username" type="text" name="username" value=username) 
    input(type="submit" class="submit" value="Add")
  a(href=`/${playlistType}/playlists/${page}/playlist/${pagePl}/${playlistId}`) Back

